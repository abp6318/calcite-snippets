  "Calcite Alert Example (queuing alerts)": {
    "prefix": "calcite-alert_example_queuing-alerts",
    "description": "Alerts are contextual pop-up style messages that can contain actions without obstructing the application.",
    "body": [
      "<calcite-alert id=\"original-alert\" kind=\"success\" open icon=\"check-layer\" label=\"2010 County Census Report saved\">",
      "\t<div slot=\"title\">Layer saved</div>",
      "\t<div slot=\"message\">",
      "\t\tFeature <b>2010 County Census Report</b> was added to the map",
      "\t</div>",
      "</calcite-alert>",
      "<calcite-button id=\"example-button\" text=\"Open modal\" icon-start=\"plus\">",
      "\tAdd alert to queue",
      "</calcite-button>",
      "<script>",
      "\tconst originalAlert = document.getElementById(\"original-alert\");",
      "\tconst button = document.getElementById(\"example-button\");",
      "",
      "\tbutton.addEventListener(\"click\", function() {",
      "\t\t// create a new alert",
      "\t\tconst exampleAlert = [",
      "\t\t\t\"<calcite-alert kind='danger' open>\",",
      "\t\t\t\"<div slot='title'>Something failed</div>\",",
      "\t\t\t\"<div slot='message'>That thing you wanted to do didn't work as expected</div>\",",
      "\t\t\t\"</calcite-alert>\"",
      "\t\t].join(\"\\n\");",
      "\t\t// add the created alert",
      "\t\toriginalAlert.insertAdjacentHTML(\"afterend\", exampleAlert);",
      "\t});",
      "</script>"
    ]
  },
