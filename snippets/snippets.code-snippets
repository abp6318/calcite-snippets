      "\t\t\t\t\t<calcite-list>",
      "\t\t\t\t\t\t<calcite-list-item label=\"Narnia Community College\" description=\"Wardobe, IA\">",
      "\t\t\t\t\t\t</calcite-list-item>",
      "\t\t\t\t\t\t<calcite-list-item label=\"University of Acme\" description=\"Acmeton, CA\"></calcite-list-item>",
      "\t\t\t\t\t\t<calcite-list-item label=\"Roadrunner Trade School\" description=\"Zion, UT\">",
      "\t\t\t\t\t\t</calcite-list-item>",
      "\t\t\t\t\t\t<calcite-list-item label=\"Cartographic Institute\" description=\"Redlands, CA\">",
      "\t\t\t\t\t\t</calcite-list-item>",
      "\t\t\t\t\t</calcite-list>",
      "\t\t\t\t</calcite-block>",
      "\t\t\t</calcite-flow-item>",
      "\t\t</calcite-flow>",
      "\t</calcite-shell-panel>",
      "\t<calcite-panel heading=\"Content\"></calcite-panel>",
      "</calcite-shell>",
      "<script>",
      "\tconst flow = document.getElementById(\"example-flow\");",
      "\tconst items = document.querySelectorAll(\"calcite-list-item\");",
      "",
      "\titems?.forEach(item => {",
      "\t\titem.addEventListener(\"calciteListItemSelect\", event => {",
      "\t\t\tcreateFlowItem(event, event.target.label, event.target.description, false);",
      "\t\t});",
      "\t});",
      "",
      "\tfunction createFlowItem(event, title, description, isLastLevel) {",
      "\t\tconst newFlowItem = document.createElement(\"calcite-flow-item\");",
      "\t\tnewFlowItem.heading = !isLastLevel ? title : \"Even more details\";",
      "\t\tnewFlowItem.description = !isLastLevel ? description : title;",
      "",
      "\t\tconst block = document.createElement(\"calcite-block\");",
      "\t\tblock.open = true;",
      "\t\tblock.heading = \"Details\";",
      "\t\tnewFlowItem.append(block);",
      "",
      "\t\tconst notice = document.createElement(\"calcite-notice\");",
      "\t\tnotice.open = true;",
      "\t\tnotice.width = \"full\";",
      "\t\tblock.append(notice);",
      "",
      "\t\tconst noticeMessage = document.createElement(\"span\");",
      "\t\tnoticeMessage.slot = \"message\";",
      "\t\tnoticeMessage.innerText = !isLastLevel ? `A new Flow Item for ${title}.` :",
      "\t\t\"You've reached the end of the line.\";",
      "\t\tnotice.append(noticeMessage);",
      "",
      "\t\tif (!isLastLevel) {",
      "\t\t\tconst button = document.createElement(\"calcite-button\");",
      "\t\t\tbutton.slot = \"footer\";",
      "\t\t\tbutton.width = \"full\";",
      "\t\t\tbutton.innerText = \"Move to a third Flow Item\";",
      "\t\t\tbutton.addEventListener(\"click\", event => createFlowItem(event, title, description, true));",
      "\t\t\tif (!isLastLevel) newFlowItem.append(button);",
      "\t\t}",
      "",
      "\t\tflow.append(newFlowItem);",
      "\t}",
      "</script>"
    ]
  },
